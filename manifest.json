{
    "manifest_version": 2,

    "name": "SQL Format",
    "description": "A chrome plugin to format your SQL code",
    "version": "2.0",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "storage"
    ],

    "options_page": "options.html"
}
