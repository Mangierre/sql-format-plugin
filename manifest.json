{
    "manifest_version": 2,

    "name": "SQL Format",
    "description": "A chrome plugin to format your SQL code",
    "version": "1.3",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}

